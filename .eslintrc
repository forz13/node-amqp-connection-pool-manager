{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "no-empty": "warn",
    "eqeqeq": "warn",
    "consistent-return": "off",
    "no-shadow": "off",
    "no-underscore-dangle": "off",
    "radix": "off",
    "no-restricted-globals": "off",
    "no-restricted-syntax": "off",
    "no-continue": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "global-require": "off",
    "func-names": "off",
    "no-plusplus": "off",
    "no-param-reassign": "off",
    "prefer-rest-params": "off",
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "no-unused-vars": "warn",
    "class-methods-use-this": "off",
    "prefer-destructuring": [
      "error",
      {
        "VariableDeclarator": {
          "array": false,
          "object": true
        }
      }
    ]
  }
}